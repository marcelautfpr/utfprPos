@Article{actascianimsci10755,
  author        = {Jayme Povh and Héden Moreira and Ricardo Ribeiro and Alberto Prioli and Lauro Vargas and Danielly Blanck and Eliane Gasparino and Danilo Streit { } Jr},
  title         = {Estimativa da variabilidade genética em linhagens de tilápia do Nilo (Oreochromis niloticus) com a técnica de RAPD},
  journal       = {Acta Scientiarum. Animal Sciences},
  year          = {2005},
  volume        = {27},
  number        = {1},
  pages         = {1-10},
  issn          = {1807-8672},
  url           = {http://revistas.bvs-vet.org.br/actascianimsci/article/view/10755},
  abstract      = {A variabilidade genética é essencial para que se possa obter melhoramento genético e, portanto, é de grande importância a sua estimação. Desta forma, o objetivo do presente experimento foi estimar, pela técnica Random Amplified Polymorphic DNA (RAPD), a divergência e a variabilidade genética nas linhagens de tilápias do Nilo (Oreochromis niloticus) Bouaké e Chitralada em duas gerações de reprodutores do rio Nilo. Foram utilizados 20 animais de cada linhagem. A matriz de coeficientes de similaridade de Jaccard entre indivíduos foi utilizada para a construção de um dendrograma e para a determinação, com o teste de Mantel, da divergência genética entre linhagens. A variabilidade genética foi estimada pelo índice de Shannon e pela porcentagem de loci polimórficos. As linhagens Bouaké e Chitralada formaram grupos distintos. A primeira apresentou menor divergência e variabilidade genética em relação à segunda. A variabilidade genética foi semelhante entre as duas gerações de reprodutores em ambas as linhagens},
  keywords      = {divergência genética; melhoramento genético; Oreochromis niloticus; RAPD; polimorfismo; variabilidade genética},
  timestamp     = {24/04/2017},
  urlaccessdate = {25 out. 2016},
}

